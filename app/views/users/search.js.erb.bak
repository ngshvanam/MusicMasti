$("#grooveshark_loading").css("display","none");
$("#grooveshark_result_count").text("<%=@playlist.count%> Results");
<%if @playlist.count == 0 %>
$("#grooveshark_results").html("<p>No results found.</p>");
<%else%>
$("#grooveshark_results").html("<ul id='grooveshark_result_list' class='result-list'></ul>");
<%end%>
<%@playlist.each do |song| %>
 $("#grooveshark_result_list").append("<li>\
				<a href='<%=song['Url']%>' target='m'><%=song['ArtistName']%> - <%=song['SongName']%></a>\
				<span class='album'><%=song['AlbumName']%></span>\
			</li>");
<%end%>
